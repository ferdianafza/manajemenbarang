<header class="w3-container" style="padding-top:22px">
  <!-- <h5><b><i class="fa fa-dashboard"></i> Dashboard</b></h5> -->
  <br>
</header>

<div class="w3-row-padding w3-margin-bottom">
  <div class="w3-quarter">
    <div class="w3-container w3-red w3-padding-16">
      <div class="w3-left"><i class="fa-solid fa-cubes w3-xxxlarge"></i></div>
      <div class="w3-right">
        <h3><%= @barangKeluarPerHari %></h3>
      </div>
      <div class="w3-clear"></div>
      <h4>Barang Keluar Per Hari</h4>
    </div>
  </div>
  <div class="w3-quarter">
    <div class="w3-container w3-blue w3-padding-16">
      <div class="w3-left"><i class="fa-solid fa-right-to-bracket w3-xxxlarge"></i></div>
      <div class="w3-right">
        <h3><%= @barangKeluarPerMinggu %></h3>
      </div>
      <div class="w3-clear"></div>
      <h4>Barang Keluar Perminggu</h4>
    </div>
  </div>
  <div class="w3-quarter">
    <div class="w3-container w3-teal w3-padding-16">
      <div class="w3-left"><i class="fa-solid fa-right-from-bracket w3-xxxlarge"></i></div>
      <div class="w3-right">
        <h3><%= @barangKeluarPerBulan %></h3>
      </div>
      <div class="w3-clear"></div>
      <h4>Barang Keluar Perbulan</h4>
    </div>
  </div>
  <div class="w3-quarter">
    <div class="w3-container w3-orange w3-text-white w3-padding-16">
      <div class="w3-left"><i class="fa-regular fa-circle-xmark w3-xxxlarge"></i></div>
      <div class="w3-right">
        <h3><%= @barangKeluarPerTahun %></h3>
      </div>
      <div class="w3-clear"></div>
      <h4>Barang Keluar Pertahun</h4>
    </div>
  </div>
</div>


<div class="w3-panel">
  <div class="w3-row-padding" style="margin:0 -16px">
    <%= form_tag barang_keluars_path, :method => 'get' do %>
      <p>
        <div class="input-group">
          <%= date_field_tag :created_at, params[:created_at], class: "form form-control input-70", placeholder: "Pilih Tanggal" %>
          <span class="input-group-btn">
            <%= submit_tag "Search", name: nil, class: "btn btn-info" %>
          </span>
        </div>
      </p>
    <% end %>
    <%= render "listbarangkeluar", barang: @listBarangKeluar %>
  </di
